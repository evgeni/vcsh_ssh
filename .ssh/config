HashKnownHosts no

Host *
	ControlMaster auto
	ControlPath ~/.ssh/master-%r@%h:%p

Host rofl.geekosphere.org
	User zhenech

Host *.debian.org *.debian.net
	User evgeni
	IdentityFile ~/.ssh/debian_rsa

Host git2.debian.org
	ProxyCommand ssh master.debian.org -W %h:%p


Host *.grml.org
	User evgeni
	IdentityFile ~/.ssh/grml_rsa

Host dorei
	User evgeni
	HostName dorei.kerker.die-welt.net

Host repo.or.cz
	User evgeni

Host *.sourceforge.net
	User sargentd

Host ubuntu
	Hostname 192.168.122.15

Host lxc-*
	ProxyCommand ssh ubuntu sudo lxc-attach -n $(echo %h|sed 's/lxc-//') nc localhost 22

Host jump.ci.centos.org
 Hostname jump.ci.centos.org
 User evgeni
 ProxyCommand none
 ForwardAgent yes
 GSSAPIAuthentication no

Host *.ci.centos.org
  ProxyCommand ssh jump.ci.centos.org -W %h:%p
  ForwardAgent yes
  GSSAPIAuthentication no
